{
  "packageManager": "yarn@1.22.19",
  "name": "backup-storage",
  "description": "",
  "license": "MIT",
  "scripts": {
    "fix:lint": "eslint --fix .",
    "fix:prettier": "prettier -w ./src .",
    "test:lint": "eslint .",
    "test:prettier": "prettier -c ./src",
    "test:tsc": "tsc --noEmit",
    "db:migrate": "sequelize-cli db:migrate",
    "build:static": "cp -r ./src/views/static ./dist/views/ && cp -r ./src/cert/ ./dist/ && cp -r ./src/migrations/ ./dist/",
    "build:css": "tailwindcss -i ./src/views/static/css/index.css -o ./src/views/static/css/style.css",
    "build": "tsc && npm run build:css && npm run build:static",
    "start:prod": "pm2 start ./dist/main.js",
    "start": "pm2 start ./dist/main.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jsxte": "^2.3.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.7",
    "pm2": "^5.3.1",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fix-tsc-es-imports": "^0.1.6",
    "prettier": "^2.8.4",
    "prettier-plugin-jsdoc": "^0.4.2",
    "sequelize-cli": "^6.6.2",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.2",
    "typescript": "^4.9.5"
  }
}
